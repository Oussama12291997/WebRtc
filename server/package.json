{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "devDependencies": {
    "tslint": "^6.1.3",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "prebuild": " tslint -c tslint.json -p tsconfig.json --fix",
    "build": "yarn tsc",
    "watch": "yarn tsc -w",
    "prestart": "yarn build",
    "start": "concurrently --kill-others \"yarn watch\" \"yarn nodemon .\" "
  },
  "dependencies": {
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.7"
  }
}
